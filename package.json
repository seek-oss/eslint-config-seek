{
  "name": "eslint-config-seek",
  "version": "0.0.0-development",
  "description": "ESLint configuration used by SEEK",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seek-oss/eslint-config-seek.git"
  },
  "author": "Seek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seek-oss/eslint-config-seek/issues"
  },
  "scripts": {
    "test": "eslint --config 'index.js' .",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit --extends seek"
    }
  },
  "homepage": "https://github.com/seek-oss/eslint-config-seek#readme",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-css-modules": "^2.9.1",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "find-root": "^1.1.0",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "commitizen": "^3.0.2",
    "commitlint-config-seek": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.7.0",
    "husky": "^1.1.2",
    "semantic-release": "^15.9.17",
    "travis-deploy-once": "^5.0.9"
  },
  "release": {
    "success": false
  },
  "peerDependencies": {
    "eslint": ">=5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
