{
  "name": "eslint-config-seek",
  "version": "0.0.0-development",
  "description": "ESLint configuration used by SEEK",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/seek-oss/eslint-config-seek.git"
  },
  "author": "Seek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seek-oss/eslint-config-seek/issues"
  },
  "scripts": {
    "test": "eslint . --ignore-pattern '!.eslintrc.js'",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit --extends seek"
    }
  },
  "homepage": "https://github.com/seek-oss/eslint-config-seek#readme",
  "dependencies": {
    "@typescript-eslint/parser": "^1.12.0",
    "babel-eslint": "^10.0.2",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-cypress": "^2.6.0",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.10.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "find-root": "^1.1.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "commitizen": "^3.1.2",
    "commitlint-config-seek": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.0.1",
    "husky": "^3.0.0",
    "semantic-release": "^15.13.18",
    "travis-deploy-once": "^5.0.11"
  },
  "release": {
    "success": false
  },
  "peerDependencies": {
    "eslint": ">=5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
